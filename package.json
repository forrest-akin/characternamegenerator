{
  "name": "random-character-name",
  "version": "1.5.3",
  "description": "It will generate random names for stories",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test:watch": "npm t -- -w",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmichelin/characternamegenerator.git"
  },
  "keywords": [
    "random",
    "name",
    "generator"
  ],
  "files": [
    "dist",
    "data",
    "README.md",
    "CONTRIBUTING.md"
  ],
  "author": "John Michelin <john@javascriptjohn.com> (http://www.javascriptjohn.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmichelin/characternamegenerator/issues"
  },
  "homepage": "https://github.com/jmichelin/characternamegenerator#readme",
  "dependencies": {
    "lodash": "4.14.1",
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.11.6",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.5",
    "cz-conventional-changelog": "1.1.7",
    "ghooks": "1.3.2",
    "istanbul": "0.4.4",
    "mocha": "3.0.0",
    "nyc": "8.1.0",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
